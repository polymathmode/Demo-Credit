{
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc",
    "migrate": "npx ts-node -r tsconfig-paths/register ./node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.js",
    "rollback": "npx ts-node -r tsconfig-paths/register ./node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile.js",
    "dev": "nodemon --exec ts-node src/index.ts"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
